# Environment variables for The Urim dApp

# LLM API Keys
OPENAI_API_KEY=your_openai_key_here
NEXT_PUBLIC_OPENROUTER_API_KEY=your_openrouter_key_here

# Wallet Data APIs
MORALIS_API_KEY=your_moralis_key_here
ALCHEMY_API_KEY=your_alchemy_key_here

# Market Data APIs
COINGECKO_API_KEY=your_coingecko_key_here
COINMARKETCAP_API_KEY=your_coinmarketcap_key_here

# Opik Configuration
OPIK_API_KEY=your_opik_key_here
OPIK_PROJECT_NAME=the-urim

# Blockchain Configuration
POLYGON_TESTNET_RPC_URL=https://polygon-mumbai.infura.io/v3/YOUR_INFURA_KEY
PRIVATE_KEY=your_private_key_here

# MetaMask SDK Configuration
DISABLE_METAMASK_ANALYTICS=true
DISABLE_METAMASK_TELEMETRY=true
